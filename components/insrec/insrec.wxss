/* components/insrec/insrec.wxss */
.inspect-list {
  padding: 25rpx;
}

.inspect-item {
  color: #FFFFFF;
  background: url("data:image/png;base64,") no-repeat;
  width: 100%;
  height: 400rpx;
  background-size: 100% 100%;
  margin-bottom: 20rpx;
  display: grid;
  grid-template-columns: 2fr 1fr;
  grid-template-rows: 273rpx auto;
}

.inspect-item>view {
  height: 240rpx;
}

.item-left {
  /* background-color: #09BB07; */
  padding: 20rpx;
}

.item-left view {
  margin-bottom: 5rpx;
}

.item-bottom {
  /* background-color: #8379EF; */
  height: 127rpx !important;
  grid-column-start: 1;
  grid-column-end: 3;
  position: relative;
  /* border-top: 1px #FFFFFF dashed; */
}

.item-bottom::after {
  content: "";
  position: absolute;
  border-top: 2px #FFFFFF dashed;
  width: 90%;
  /* height: 1px; */
  top: -1px;
  left: 50%;
  transform: translate(-50%);
}

.item-left image {
  width: 50rpx;
  height: 50rpx;
  vertical-align: bottom;
  margin-right: 15rpx;
}

.marginright {
  margin-right: 50rpx;
}

.item-right {
  display: flex;
  justify-content: center;
  align-items: center;
}

.item-right image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: #FF4603;
}

.item-right text {
  font-size: 50rpx;
}

.item-right>view {
  display: inline-flex;

  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: #7EB9FF;

  background-color: #7EB9FF;
  padding: 15rpx 55rpx;
  border-radius: 10px;
}

.item-bottom {
  padding: 20rpx;
  margin-bottom: 5rpx;
}