
var imageUrl = function (imgUrl) {
  var str=imgUrl
  var i = str.indexOf('"')
  var k = str.lastIndexOf('"')
  str = str.slice(i + 1, k)
  var http = 'https://img.ego360.cn'
  str=str.replace('http://{0}',http)
  return str
}

