.header{
  width: 100%;
  position: fixed;
  z-index: 10;
  border: 1px solid transparent;
}
.van-tabbar--fixed{
  top: 0!important;
}
.content{
  /* margin-top: 150rpx; */
  margin-bottom: 20rpx;
  /* height: 600rpx; */
  padding: 0 20rpx;
}

/* //订单 */
.van-sticky{
  position: fixed!important;
  width: 100vw;
  z-index: 500;
}
.c-title{
  display: flex;
  justify-content: space-between;
  height: 86rpx;
  line-height: 86rpx;
  margin-top: 88rpx;
  color: #808080;
  border-bottom: 1rpx solid #D2D2D2;
}
.c-title view text:nth-child(1){
  font-size: 45rpx;
  margin-right: 20rpx;
}
.SellerName{
  width: 501prx;
  height: 50rpx;
  overflow:hidden; 
text-overflow:ellipsis;
white-space:no-wrap;
}
.c-goods{
  position: relative;
  display: flex;
  margin-top: 10rpx;
  padding-bottom: 5rpx;
  border-bottom: 1rpx solid #F4F4F4;
}

.c-goods view:nth-child(1) image{
  width: 180rpx;
  height: 180rpx;
  border-radius: 10rpx;
}
.c-goodsinfo{
  position: absolute;
  top: 51rpx;
  left: 184rpx;
}
.price{
  position: absolute;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  top: 105rpx;
  color: #999999;
  right: 35rpx;
}
.c-goods view:nth-child(2) {
  height: 80rpx;
  margin-left: 10rpx;
}
.c-goods view:nth-child(2) view{
  height: 50rpx;  
  
}
.c-goods view:nth-child(2) view:nth-child(1){
  font-size: 30rpx;
  color: #333333;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.c-goods view:nth-child(2) view:nth-child(2){
  color: #787878;
}
.footer{
  display: flex;
  height: 86rpx;
  line-height: 86rpx;
  border-bottom: 1rpx solid #F4F4F4 ;
}
.footer view:nth-child(1){
  margin-right: 148rpx;
  color: #999999;
}
.footer view:nth-child(2){
  margin-right: 20rpx;
}
.footer view:nth-child(2) image{
  margin-top: 18rpx;
  width: 45rpx;
  height: 45rpx;
  margin-right: 10rpx;
}
.footer view:nth-child(2) text{
  color: #999999;
}
.footer view:nth-child(2){
  display: flex;
}
.footer view:nth-child(3) text{
  color: #999999;
}
.monny{
